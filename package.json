{
  "name": "amir-houieh-portfolio",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "stage": "react-static build --staging",
    "build": "yarn data:build && react-static build",
    "serve": "serve dist -p 3000",
    "data:build": "yarn clean-up && ts-node --project tsconfig-data.json data-module/index",
    "data:sandbox": "ts-node --project tsconfig-data.json data-module/sandbox",
    "data:dev": "ts-node --project tsconfig-data.json data-module/index",
    "clean-up": "rm -rf public/images && mkdir public/images && rm -rf public/data.json"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "html2plaintext": "^2.1.0",
    "lodash": "^4.17.11",
    "marked": "^0.7.0",
    "promise-sequential": "^1.1.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-player": "^1.9.3",
    "react-router": "^4.2.0",
    "react-static": "^5.9.12",
    "sharp": "^0.24.0",
    "slugify": "^1.3.3",
    "uniqid": "^5.0.3",
    "url-join": "^4.0.0",
    "webloc-parser": "^0.1.1",
    "yaml-front-matter": "^4.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/lodash": "^4.14.149",
    "@types/marked": "^0.4.2",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-router": "^4.0.16",
    "@types/sharp": "^0.24.0",
    "@types/uniqid": "^4.1.2",
    "@types/webpack-env": "^1.13.5",
    "babel-polyfill": "^6.26.0",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "eslint-config-react-tools": "1.1.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "nodemon": "^1.18.5",
    "serve": "^6.5.3",
    "ts-loader": "^3.5.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
